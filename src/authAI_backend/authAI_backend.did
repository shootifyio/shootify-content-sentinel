type StoredImage = record {
    content: blob;
    uploaded_by: principal;
};

service : {
    store_image: (text, blob) -> (variant { Ok; Err: text });
    get_image: (text) -> (opt StoredImage) query;
    list_images: () -> (vec text) query;
    delete_image: (text) -> (variant { Ok; Err: text });
};
